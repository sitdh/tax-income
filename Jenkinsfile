pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Build Jar file'
      }
    }
    stage('Instrumatation') {
      steps {
        echo 'Instrumenting source code'
      }
    }
    stage('Collecting constants') {
      steps {
        echo 'Collecting constants from source code'
      }
    }
  }
}
